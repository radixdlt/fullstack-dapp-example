{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        ".svelte-kit/**"
      ],
      "cache": false
    },
    "lint": {},
    "format": {},
    "svelte:check": {},
    "test": {},
    "test:integration": {
      "dependsOn": [
        "^build"
      ],
      "cache": false
    },
    "dev": {
      "dependsOn": [
        "^build"
      ],
      "cache": false,
      "persistent": true
    },
    "dev:inspect": {
      "dependsOn": [
        "^build"
      ],
      "cache": false,
      "persistent": true
    },
    "generate": {
      "cache": false
    },
    "push": {
      "cache": false
    },
    "wallet:create:mnemonic": {
      "cache": false
    },
    "wallet:create:badges": {
      "cache": false
    },
    "wallet:create:element": {
      "cache": false
    },
    "wallet:list:accounts": {
      "cache": false
    },
    "wallet:faucet": {
      "cache": false
    },
    "wallet:mint-admin-badge": {
      "cache": false
    },
    "wallet:mint-user-badge": {
      "cache": false
    },
    "build:storybook": {
      "cache": false
    }
  }
}
