{
  "$schema": "https://turbo.build/schema.json",
  "globalEnv": [
    "REDIS_HOST",
    "REDIS_PORT",
    "REDIS_PASSWORD",
    "POSTGRES_PASSWORD",
    "POSTGRES_USER",
    "POSTGRES_DB",
    "NOTIFICATION_INTERNAL_API_PORT",
    "NOTIFICATION_WEBSOCKET_PORT",
    "NOTIFICATION_INTERNAL_API_HOST",
    "NOTIFICATION_WEBSOCKET_HOST",
    "NOTIFICATION_INTERNAL_API_URL",
    "WEB_API_PORT",
    "POSTGRES_DATABASE",
    "POSTGRES_HOST",
    "POSTGRES_USER",
    "POSTGRES_PASSWORD",
    "POSTGRES_PORT",
    "EXPECTED_ORIGIN",
    "JWT_SECRET",
    "PUBLIC_NETWORK_ID",
    "PUBLIC_NOTIFICATION_URL",
    "LOG_LEVEL",
    "PUBLIC_LOG_LEVEL",
    "PUBLIC_RADQUEST_API_URL",
    "MNEMONIC",
    "TWILIO_AUTH_TOKEN",
    "TWILIO_ACCOUNT_SID",
    "TWILIO_SERVICE_SID",
    "NGROK_AUTH_TOKEN",
    "DATABASE_URL"
  ],
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".svelte-kit/**"],
      "cache": false
    },
    "lint": {
      "cache": false
    },
    "format": {},
    "svelte:check": {},
    "test": {},
    "test:ci": {},
    "test:integration": {
      "dependsOn": ["^build"],
      "cache": false,
      "persistent": true,
      "inputs": ["$TURBO_DEFAULT$"]
    },
    "dev": {
      "dependsOn": ["^build"],
      "cache": false,
      "persistent": true,

      "inputs": ["$TURBO_DEFAULT$"]
    },
    "dev:inspect": {
      "dependsOn": ["^build"],
      "cache": false,
      "persistent": true
    },
    "generate": {
      "cache": false
    },
    "push": {
      "cache": false
    },
    "wallet:create:mnemonic": {
      "cache": false
    },
    "wallet:create:badges": {
      "cache": false
    },
    "wallet:create:element": {
      "cache": false
    },
    "wallet:list:accounts": {
      "cache": false
    },
    "wallet:faucet": {
      "cache": false
    },
    "wallet:mint-admin-badge": {
      "cache": false
    },
    "wallet:mint-hero-badge": {
      "cache": false
    },
    "build:storybook": {
      "cache": false
    },
    "tsc": {
      "cache": false
    }
  }
}
