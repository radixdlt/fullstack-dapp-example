{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^db:build"],
      "outputs": [".svelte-kit/**"]
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "test": { "dependsOn": ["^db:build"] },
    "dev": {
      "dependsOn": ["^db:generate"],
      "cache": false,
      "persistent": true
    },
    "dev:inspect": {
      "dependsOn": ["^db:generate"],
      "cache": false,
      "persistent": true
    },
    "db:generate": {
      "cache": false
    },
    "db:push": {
      "cache": false
    },
    "db:build": {
      "cache": false
    }
  }
}
