{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["db:generate", "queues:build"],
      "outputs": [".svelte-kit/**"],
      "cache": false
    },
    "lint": {
      "dependsOn": ["^lint", "^common:build"]
    },
    "format": {},
    "svelte:check": {},
    "test": { "dependsOn": ["^db:build"], "cache": false },
    "test:integration": { "dependsOn": ["^db:build", "^common:build", "^wallet:build", "^content:build", "^queues:build"], "cache": false },
    "dev": {
      "dependsOn": ["^db:generate", "^common:build", "^content:build", "^queues:build"],
      "cache": false,
      "persistent": true
    },
    "dev:inspect": {
      "dependsOn": ["^db:generate"],
      "cache": false,
      "persistent": true
    },
    "db:generate": {
      "cache": false
    },
    "db:push": {
      "cache": false
    },
    "db:build": {
      "cache": false
    },
    "wallet:create:mnemonic": {
      "cache": false
    },
    "wallet:create:badges": {
      "cache": false
    },
    "wallet:create:element": {
      "cache": false
    },
    "wallet:list:accounts": {
      "cache": false
    },
    "wallet:faucet": {
      "cache": false
    },
    "wallet:mint-admin-badge": {
      "cache": false
    },
    "wallet:mint-user-badge": {
      "cache": false
    },
    "wallet:build": {
      "cache": false
    },
    "common:build": {
      "cache": false
    },
    "content:build": {
      "dependsOn": ["^common:build"],
      "cache": false
    },
    "queues:build": {
      "cache": false
    }
  }
}
