{
  "name": "admin",
  "version": "0.0.1",
  "main": "dist/index.js",
  "private": true,
  "scripts": {
    "dev": "vite dev --port 5175 | npx pino-pretty",
    "build": "vite build",
    "preview": "vite preview",
    "test": "npm run test:unit",
    "svelte:check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "lint": "prettier --check . && eslint . && npm run svelte:check",
    "format": "prettier --write .",
    "test:unit": "vitest --passWithNoTests",
    "gen:exports": "svelte-lib-helpers exports",
    "gen:docs": "svelte-lib-helpers docs",
    "gen:compo-data": "svelte-lib-helpers compo-data",
    "copy:package": "svelte-lib-helpers package",
    "lib-helpers": "npm run gen:docs && npm run gen:compo-data && npm run build && npm run gen:exports && npm run copy:package",
    "package:publish": "standard-version && git push --follow-tags origin main && npm publish"
  },
  "devDependencies": {
    "@sveltejs/adapter-node": "^2.0.0",
    "@sveltejs/kit": "^2.5.9",
    "@sveltejs/vite-plugin-svelte": "^3.1.0",
    "@types/eslint": "8.56.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "autoprefixer": "^10.4.19",
    "dayjs": "^1.11.11",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.39.0",
    "postcss": "^8.4.38",
    "postcss-load-config": "^5.1.0",
    "prettier": "^3.2.5",
    "prettier-plugin-svelte": "^3.2.3",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "svelte": "^4.2.17",
    "svelte-check": "^3.7.1",
    "svelte-lib-helpers": "^0.3.17",
    "svelte-meta-tags": "^3.1.2",
    "tailwindcss": "^3.4.3",
    "tslib": "^2.6.2",
    "typescript": "^5.4.5",
    "vite": "^5.2.11",
    "vitest": "^1.6.0"
  },
  "type": "module",
  "peerDependencies": {
    "svelte": "^4.0.0"
  },
  "dependencies": {
    "@radixdlt/radix-dapp-toolkit": "^2.0.0-v2.42",
    "common": "*",
    "database": "*",
    "flowbite-svelte": "^0.44.24",
    "flowbite-svelte-icons": "^1.6.1",
    "jsonwebtoken": "^9.0.2",
    "queues": "*",
    "tailwind-merge": "^2.3.0"
  }
}
