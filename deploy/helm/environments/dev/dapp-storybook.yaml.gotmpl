image: 
    tag: {{ .StateValues.ci.tag }}

params:

secrets:

ingress:
  enabled: false
  basic_auth:
    enabled: true
    secret_name: rdx-works-main/eks/radquest/temp-basic-auth
  annotations:
    kubernetes.io/ingress.class: nginx
    nginx.ingress.kubernetes.io/auth-type: basic
    nginx.ingress.kubernetes.io/auth-secret: dapp-storybook-basic-auth
    nginx.ingress.kubernetes.io/auth-realm: 'Auth Required'
  hosts:
    - host: radquest-storybook-dev.rdx-works-main.extratools.works
      paths:
        - path: /
          pathType: ImplementationSpecific

autoscaling:
  enabled: false

resources:
  requests:
    cpu: 50m
    memory: 128Mi
