    aws_secret_key: "jwt_secret"
    secret_key: "jwt_secret"

ingress:
  enabled: true
  annotations:
    kubernetes.io/ingress.class: nginx
  hosts:
    - host: fullstack-dapp-example.radixdlt.com
      paths:
