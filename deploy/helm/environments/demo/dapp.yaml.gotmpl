    aws_secret_name: "rdx-works-main-dev/eks/fullstack-dapp-example/demo/secrets"
    aws_secret_key: "ro-database-url"
    secret_key: "ro-database-url"

  - external_secret_name: dapp-jwt-secret
    env_variable: "JWT_SECRET"
